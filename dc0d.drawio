        <mxCell id="Uje2sW3toLFuQxsrkGGJ-1" value="if (p.wallet &amp;gt;= weapon.cost) {&lt;br&gt;&amp;nbsp; send (&quot;&quot;, true)&lt;br&gt;} else {&lt;br&gt;&amp;nbsp; send (&quot;&quot;, false)&lt;br&gt;}" style="rounded=0;whiteSpace=wrap;html=1;dashed=1;fillColor=#f9f7ed;strokeColor=#36393d;align=left;verticalAlign=top;" vertex="1" parent="Qu2Mb2IjLPH-KH6Ql788-1">
